{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
      "publisherEmail": {
        "type": "string",
        "metadata": {
          "description": "Publisher email"
        }
      },
      "publisherName": {
        "type": "string",
        "metadata": {
          "description": "Publisher name"
        }
      },
      "apimServiceName": {
        "type": "string",
        "metadata": {
          "description": "Name of the API Management gateway"
        }
      },
      "sku": {
        "type": "string",
        "allowedValues": [ "Developer", "Standard", "Premium" ],
        "defaultValue": "Developer",
        "metadata": {
          "description": "The pricing tier of this API Management service"
        }
      },
      "skuCount": {
        "type": "int",
        "defaultValue": 1,
        "metadata": {
          "description": "The instance  size of this API Management service."
        }
      },
      "_artifactsLocation": {
        "type": "string",
        "metadata": {
          "description": "The base URI where artifacts required by this template are located including a trailing '/'"
        },
        "defaultValue": ""
      },
      "_artifactsLocationSasToken": {
        "type": "securestring",
        "metadata": {
          "description": "The sasToken required to access _artifactsLocation.  When the template is deployed using the accompanying scripts, a sasToken will be automatically generated. Use the defaultValue if the staging location is not secured."
        },
        "defaultValue": ""
      }
    },
    "functions": [],
    "variables": {},
    "resources": [
      {
        "apiVersion": "2020-06-01",
        "name": "serviceTemplate",
        "type": "Microsoft.Resources/deployments",
        "properties": {
          "mode": "Incremental",
          "templateLink": {
            "uri": "[concat(parameters('_artifactsLocation'), 'service.template.json', parameters('_artifactsLocationSasToken'))]",
            "contentVersion": "1.0.0.0"
          },
          "parameters": {
            "apimServiceName": { "value": "[parameters('apimServiceName')]" }
          }
        }
      },
      {
        "apiVersion": "2020-06-01",
        "name": "allAPIs",
        "type": "Microsoft.Resources/deployments",
        "properties": {
          "mode": "Incremental",
          "templateLink": {
              "uri":"[concat(parameters('_artifactsLocation'), 'rest.template.json', parameters('_artifactsLocationSasToken'))]",
              "contentVersion":"1.0.0.0"
          },
          "parameters": {
              "apimServiceName": {"value": "[parameters('apimServiceName')]" }
            }
        },
        "dependsOn": [
            "serviceTemplate"
        ]
      }
    ],
    "outputs": {}
  }